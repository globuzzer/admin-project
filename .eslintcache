FileChange = async (e) => {\n    const file = e.target.files[0];\n    const storageRef = app.storage().ref();\n    const fileRef = storageRef.child(file.name);\n    await fileRef.put(file);\n    setImages(await fileRef.getDownloadURL());\n  };\n\n  //updating the items in firestore\n  const updateItem = (id) => {\n    // db.doc(`features/${id}`).update({\n    firestore.collection(\"features\").doc(id).update({\n      image: images,\n      title: data.title,\n      text: data.body,\n    });\n  };\n\n  //update information when user clicks Apply\n  const updateHomeValue = () => {\n    const newData = [...homeData];\n    const id = newData[showFeature - 1].id;\n    newData[showFeature - 1].title = data.title;\n    newData[showFeature - 1].image = images;\n    newData[showFeature - 1].text = data.body;\n    //these are the updated values when user clicks apply\n    setHomeData(newData);\n    console.log(\"Check\", newData);\n    setShowFeature(false);\n    //updating the id in firestore\n    updateItem(id);\n  };\n\n  //cancel information when user clicks Cancel\n  const cancelUpdate = () => {\n    setShowFeature(false);\n  };\n\n  return (\n    <Fragment>\n      <div className=\"feature-card\">\n        <div className=\"feature-card-container\">\n          <h4>Feature</h4>\n          <div className=\"icon-text\">\n            Icon\n            <span>(Image has to be below 200 KB and PNG/JPG format)</span>\n          </div>\n          <div className=\"form-wrapper\">\n            <div className=\"upload-btn-wrapper\">\n              <input type=\"file\" onChange={onFileChange} name=\"images\" />\n              <button className=\"btn\">Upload a file</button>\n            </div>\n            <div>\n              <p>Title</p>\n              <input\n                type=\"text\"\n                name=\"title\"\n                className=\"title-input\"\n                onChange={handleChange}\n              />\n            </div>\n            <div>\n              <p>Text</p>\n              <textarea\n                name=\"body\"\n                onChange={handleChange}\n                className=\"textarea-input\"\n              />\n            </div>\n          </div>\n        </div>\n        <div className=\"btn-container\">\n          <span>\n            <button className=\"btn-apply\" onClick={updateHomeValue}>\n              Apply\n            </button>\n          </span>\n          <div className=\"vertical\" />\n          <span>\n            <button className=\"btn-cancel\" onClick={cancelUpdate}>\n              Cancel\n            </button>\n          </span>\n        </div>\n      </div>\n    </Fragment>\n  );\n};\nexport default FeatureBox;\n","/Users/tarcea/Work/01_Globuzzer/code/admin-project/src/pages/Admin/BannerForm/UploadImage.js",[],"/Users/tarcea/Work/01_Globuzzer/code/admin-project/src/components/EditDropdown/Dropdown.js",[],"/Users/tarcea/Work/01_Globuzzer/code/admin-project/src/components/TextEdit/TextEdit.js",[],"/Users/tarcea/Work/01_Globuzzer/code/admin-project/src/components/TextEdit/Data.js",[],"/Users/tarcea/Work/01_Globuzzer/code/admin-project/src/pages/Admin/JoinCommunityForm/JoinCommunityForm.js",[],"/Users/tarcea/Work/01_Globuzzer/code/admin-project/src/components/UploadImage/UploadImage.js",[],{"ruleId":"225","replacedBy":"226"},{"ruleId":"227","replacedBy":"228"},{"ruleId":"229","severity":1,"message":"230","line":15,"column":1,"nodeType":"231","endLine":15,"endColumn":37},{"ruleId":"232","severity":1,"message":"233","line":77,"column":15,"nodeType":"234","endLine":77,"endColumn":42},{"ruleId":"232","severity":1,"message":"233","line":66,"column":15,"nodeType":"234","endLine":66,"endColumn":42},{"ruleId":"235","severity":1,"message":"236","line":4,"column":9,"nodeType":"237","messageId":"238","endLine":4,"endColumn":27},{"ruleId":"229","severity":1,"message":"239","line":5,"column":1,"nodeType":"231","endLine":19,"endColumn":3},{"ruleId":"225","replacedBy":"240"},{"ruleId":"227","replacedBy":"241"},{