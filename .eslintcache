v className={styles.left} />\n        <div className={styles.right}>\n          <div className={styles.logo}>\n            <img src={logoSmall} alt=\"logo-icon\" />\n            <img src={logo} alt=\"logo\" />\n          </div>\n          <p className={styles.text}>Sign up to be Globuzzer Admin</p>\n          <form className={styles.container}>\n            <div className={styles.signIn}>\n              <input\n                type=\"text\"\n                name=\"email\"\n                placeholder=\"Email\"\n                onChange={this.inputHandler}\n                value={email}\n                required\n              />\n              <input\n                type=\"password\"\n                name=\"password\"\n                placeholder=\"Password\"\n                onChange={this.inputHandler}\n                value={password}\n                required\n              />\n            </div>\n            <div className={styles.options}>\n              <label>\n                <input type=\"checkbox\" />\n                <span className={styles.checkmark} />\n                Remember me\n              </label>\n              <a className={styles.link}>Forgot Password?</a>\n            </div>\n          </form>\n          <button\n            type=\"submit\"\n            onClick={this.submitHandler}\n            className={styles.login}\n          >\n            SIGN UP\n          </button>\n        </div>\n      </div>\n    );\n  }\n}\n\nexport default SignIn;\n","/Users/tarcea/Work/01_Globuzzer/code/admin-project/src/pages/Admin/SignIn/SignIn.js",["193"],"import React, { Component } from \"react\";\nimport styles from \"./SignIn.module.css\";\nimport logo from \"../../../assets/TravelBlog/globuzz_logo.svg\";\nimport logoSmall from \"../../../assets/TravelBlog/globe_logo.svg\";\nimport { auth } from \"../../../utils/firebase.utils\";\nimport { Link } from \"react-router-dom\";\nclass SignIn extends Component {\n  constructor(props) {\n    super(props);\n   