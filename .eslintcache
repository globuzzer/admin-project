dleWeightChange = (e) => {\n    currentText.style.fontWeight = e.target.value\n    setDefaultWeight(e.target.value)\n  }\n\n  const handleAlignChange = (e) => {\n    currentText.style.textAlign = e.target.value\n    setDefaultAlign(e.target.value)\n  }\n\n  return (\n    <div>\n      {/*Start form for text headers edit on the banner*/}\n    <div className={edit.title} style={formTextStyle}\n    onDoubleClick={() => setShowTextForm(false)}\n    >\n    <div className={edit.arrowDown}></div>\n    <span>\n    <Dropdown items={fontSizes} defaultValue={defaultSize} styleChange={handleSizeChange}/>\n    </span>\n    <span>\n    <Dropdown items={fontWeights} defaultValue={defaultWeight} styleChange={handleWeightChange}/>\n    </span>\n    <form>\n      <input type=\"color\" value={defaultColor} name=\"style.color\" onChange={handleColorChange}/>\n    </form>\n    {/* <img src=\"/images/sizer.png\" alt=\"\" /> */}\n    \n    <Dropdown items={textAligns} defaultValue={defaultAlign} styleChange={handleAlignChange}/>\n    \n    </div>\n  {/*END form for text headers edit on the banner*/}\n    {/*Start forms for city place edit on the banner*/}\n    <div className={edit.place} style={formStyle}>\n      <p className={edit.head}>Place</p>\n      <hr color=\"#E4E4E4\" />\n      <div className={edit.formContainer}>\n        <form className={edit.form} >\n          <label htmlFor=\"color\">Color</label>\n          <input type=\"text\"\n                 value={currentPlace.color}\n                 name=\"color\"\n                 onChange={handleChange}\n                 style={{marginLeft: \"20px\"}}\n          />\n          <label htmlFor=\"text\">Text</label>\n          <input type=\"text\"\n                 value={currentPlace.text}\n                 name=\"text\"\n                 onChange={handleChange}\n                 style={{marginLeft: \"20px\"}}\n          />\n          <label htmlFor=\"link\">Link</label>\n          <p>{currentPlace.text}</p>\n          <p></p>\n          <input type=\"text\"\n                 value={currentPlace.link}\n                 name=\"link\"\n                 onChange={handleChange}\n                 style={{position:\"relative\", left:\"-45px\", width:\"120%\"}}\n          />\n        </form>\n      </div>\n      <div className={edit.command}>\n      <p id=\"apply\" onClick={handleSubmit}>Apply</p>\n      <p id=\"cancel\" onClick={() => setShowForm(false)}>Cancel</p>\n      </div>\n    </div>\n  {/*END forms for city place edit on the banner*/}\n    </div>\n  )\n}\n\nexport default BannerForm;\n","/Users/flo/Documents/admin-project/src/components/AuxService/AuxServiceSection.js",[],"/Users/flo/Documents/admin-project/src/components/JoinCity/JoinCitySection.js",[],"/Users/flo/Documents/admin-project/src/pages/Admin/BannerForm/Data.js",[],"/Users/flo/Documents/admin-project/src/pages/Admin/BannerForm/Dropdown.js",[],"/Users/flo/Documents/admin-project/src/pages/Admin/ServiceForm/ServiceForm.js",[],"/Users/flo/Documents/admin-project/src/pages/Admin/ArticleForm/ArticleForm.js",[],"/Users/flo/Documents/admin-project/src/pages/Admin/CityForm/CityForm.js",[],{"ruleId":"238","replacedBy":"239"},{"ruleId":"240","replacedBy":"241"},{"ruleId":"242","severity":1,"message":"243","line":15,"column":1,"nodeType":"244","endLine":15,"endColumn":37},{"ruleId":"245","severity":1,"message":"246","line":66,"column":15,"nodeType":"247","endLine":66,"endColumn":42},{"ruleId":"245","severity":1,"message":"246","line":77,"column":15,"nodeType":"247","endLine":77,"endColumn":42},{"ruleId":"248","severity":1,"message":"249","line":10,"column":10,"nodeType":"250","messageId":"251","endLine":10,"endColumn":23},{"ruleId":"248","severity":1,"message":"252","line":34,"column":5,"nodeType":"250","messageId":"251","endLine":34,"endColumn":18},{"ruleId":"248","severity":1,"message":"253","line":4,"column":9,"nodeType":"250","messageId":"251","endLine":4,"endColumn":27},{"ruleId":"242","severity":1,"message":"254","line":5,"column":1,"nodeT