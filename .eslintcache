ordinates, coord\n       }}\n    >\n      {props.children}\n    </EditContext.Provider>\n  );\n}\n\nexport default EditContextProvider;\n","/Users/tarcea/Work/01_Globuzzer/code/admin-project/src/redux/store.js",["227"],"import { createStore, applyMiddleware } from \"redux\";\nimport { persistStore } from 'redux-persist'\nimport logger from \"redux-logger\";\n\nimport rootReducer from \"./root-reducer\";\n\nconst middlewares = [logger];\n\n//pass all values in the middlewares array\n//more scalable\nexport const store = createStore(rootReducer, applyMiddleware(...middlewares));\n\nexport const persistor = persistStore(store);\n\nexport default { store, persistor };\n","/Users/tarcea/Work/01_Globuzzer/code/admin-project/src/redux/user/user.action.js",[],"/Users/tarcea/Work/01_Globuzzer/code/admin-project/src/utils/firebase.utils.js",[],"/Users/tarcea/Work/01_Globuzzer/code/admin-project/src/redux/user/user.selectors.js",[],"/Users/tarcea/Work/01_Globuzzer/code/admin-project/src/redux/root-reducer.js",[],"/Users/tarcea/Work/01_Globuzzer/code/admin-project/src/pages/Admin/SignIn/SignIn.js",["228"],"import React, { Component } from \"react\";\nimport styles from \"./SignIn.module.css\";\nimport logo from \"../../../assets/TravelBlog/globuzz_logo.svg\";\nimport logoSmall from \"../../../assets/TravelBlog/globe_logo.svg\";\nimport { auth } from \"../../../utils/firebase.utils\";\nimport { Link } from \"react-router-dom\";\nclass SignIn extends Component {\n  constructor(props) {\n    super(props);\n    this.state = {\n      email: \"\",\n      password: \"\",\n    };\n  }\n  inputHandler = (event) => {\n    const { value, name } = event.target;\n    this.setState({ [name]: value });\n  };\n\n  submitHandler = async (e) => {\n    e.preventDefault();\n    const { email, password } = this.state;\n    try {\n      await auth.signInWithEmailAndPassword(email, password);\n      this.setState({ email: \"\", password: \"\" });\n    } catch (error) {\n      console.log(error);\n    }\n  };\n\n  render() {\n    return (\n      <div className={styles.wrapper}>\n        <div className={styles.left} />\n        <div className={styles.right}>\n          <div className={styles.logo}>\n            <img src={logoSmall} alt=\"logo-icon\" />\n            <img src={logo} alt=\"logo\" />\n          </div>\n          <p className={styles.text}>Sign in to Globuzzer Admin</p>\n          <form className={styles.container}>\n            <div className={styles.signIn}>\n              <input\n                type=\"text\"\n                name=\"email\"\n                placeholder=\"Email\"\n                onChange={this.inputHandler}\n                value={this.state.email}\n                required\n              />\n              <input\n                type=\"password\"\n                name=\"password\"\n                placeholder=\"Password\"\n                onChange={this.inputHandler}\n                value={this.state.password}\n                required\n              />\n            </div>\n            <div className={styles.options}>\n              <label>\n                <input type=\"checkbox\" />\n                <span className={styles.checkmark} />\n                Remember me\n              </label>\n              <a className={styles.link}>Forgot Password?</a>\n            </div>\n          </form>\n          <button\n            type=\"submit\"\n            onClick={this.submitHandler}\n            className={styles.login}\n          >\n            LOGIN\n          </button>\n          <Link to=\"/signup\" className={styles.link}>\n            Don't have an account? Create now\n          </Link>\n        </div>\n      </div>\n    );\n  }\n}\n\nexport default SignIn;\n","/Users/tarcea/Work/01_Globuzzer/code/admin-project/src/pages/Admin/Dashboard/Dashboard.js",[],"/Users/tarcea/Work/01_Globuzzer/code/admin-project/src/pages/Admin/AdminLanding/AdminLanding.js",[],"/Users/tarcea/Work/01_Globuzzer/code/admin-project/src/pages/Admin/SignUp/SignUp.js",["229"],"import React, { Component } from \"react\";\nimport styles from \"./SignUp.module.css\";\nimport logo from \"../../../assets/TravelBlog/globuzz_logo.svg\";\nimport logoSmall from \"../../../assets/TravelBlog/globe_logo.svg\";\nimport { auth, createUserProfileDocument } from \"../../../utils/firebase.utils\";\nclass SignIn extends Component {\n  constructor() {\n    super();\n    this.state = {\n      email: \"\",\n      password: \"\",\n    };\n  }\n  inputHandler = (event) => {\n    const { value, name } = event.target;\n    this.setState({ [name]: value });\n  };\n\n  submitHandler = async (e) => {\n    e.preventDefault();\n    const { email, password } = this.state;\n    try {\n      const { user } = await auth.createUserWithEmailAndPassword(\n        email,\n        password\n      );\n\n      await createUserProfileDocument(user, { email });\n\n      //this clears the form\n      this.setState({\n        email: \"\",\n        password: \"\",\n      });\n    } catch (error) {\n      console.error(error);\n    }\n  };\n\n  render() {\n    const { email, password } = this.state;\n\n    return (\n      <div className={styles.wrapper}>\n        <div className={styles.left} />\n        <div className={styles.right}>\n          <div className={styles.logo}>\n            <img src={logoSmall} alt=\"logo-icon\" />\n            <img src={logo} alt=\"logo\" />\n          </div>\n          <p className={styles.text}>Sign up to be Globuzzer Admin</p>\n          <form className={styles.container}>\n            <div className={styles.signIn}>\n              <input\n                type=\"text\"\n                name=\"email\"\n                placeholder=\"Email\"\n                onChange={this.inputHandler}\n                value={email}\n                required\n              />\n              <input\n                type=\"password\"\n                name=\"password\"\n                placeholder=\"Password\"\n                onChange={this.inputHandler}\n                value={password}\n                required\n              />\n            </div>\n            <div className={styles.options}>\n              <label>\n                <input type=\"checkbox\" />\n                <span className={styles.checkmark} />\n                Remember me\n              </label>\n              <a className={styles.link}>Forgot Password?</a>\n            </div>\n          </form>\n          <button\n            type=\"submit\"\n            onClick={this.submitHandler}\n            className={styles.login}\n          >\n            SIGN UP\n          </button>\n        </div>\n      </div>\n    );\n  }\n}\n\nexport default SignIn;\n","/Users/tarcea/Work/01_Globuzzer/code/admin-project/src/redux/user/user.type.js",[],"/Users/tarcea/Work/01_Globuzzer/code/admin-project/src/redux/user/user.reducer.js",[],"/Users/tarcea/Work/01_Globuzzer/code/admin-project/src/pages/Home.js",[],"/Users/tarcea/Work/01_Globuzzer/code/admin-project/src/pages/Admin/TopNav/TopNav.js",[],"/Users/tarcea/Work/01_Globuzzer/code/admin-project/src/pages/Admin/SideNav/SideNav.js",["230"],"import React from 'react';\nimport styles from './SideNav.module.css';\nimport {Link} from 'react-router-dom';\nimport {IoMdArrowDropright} from 'react-icons/io';\n\nconst SideNav = () => {\n    return (\n        <div className={styles.wrapper}>\n            <ul className={styles.container}>\n                <Link to=\"/landing\">\n                <li className={styles.landing}>Landing page</li>\n                </Link>\n                <li className={styles.section}>Section page</li>\n                <li className={styles.service}>Service page</li>\n                <li className={styles.topic}>Topic page</li>\n                <li className={styles.other}>Other page</li>\n                <li className={styles.bottom}>Bottom area</li>\n                <li className={styles.request}>Request section</li>\n            </ul>\n        </div>\n    );\n}\n\nexport default SideNav;\n